#!/bin/bash
# this is a tq meta command
# operates tq.o and tq.pl
# {.ddf , .dsz , .ddl} =cat=> .frl =perl=> .dry => dry.fed => .dry.fed.<exe>
if [ $# -eq 0 ]; then
  echo "Usage:"
  echo "  tq target=<file>"
  exit;
fi

tgt=${1#target=}

if [ -e $tgt ]; then
  echo "Target file:"
  echo "  $tgt"
  echo "  ${tgt}.dsz"
  echo "  ${tgt}.ddf"
  echo "  ${tgt}.ddl"
  cat ${tgt}.ddl ${tgt}.ddf ${tgt}.dsz > ${tgt}.frl
  echo "Create ${tgt}.frl"
else
  echo "no target file"
fi
